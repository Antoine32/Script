a = 0
b = 0
c = 0

fn test(n, m)
    print("allo: " + n + " | " + m)
    return n > m
end

loop
    b = 0

    loop
        a = 0

        if 1
            loop
                a += 1
                print("a: " + a)

                if a > 2
                    b += 1
                    print("b: " + b)
                    break
                elif a > 0
                    print("a1")
                else
                    print("a2")
                end
            end
        else
            b += 1
        end

        if test(5, 3) && true
            print("test1")
        elif test(3, 5) && true
            print("test2")
        else
            print("test3")
        end

        if b > 2
            c += 1
            print("c: " + c)
            break
        elif b > 0
            print("b1")
        else
            print("b2")
        end
    end

    if c > 2
        print("c0")
        break
    elif c > 0
        print("c1")
    else
        print("c2")
    end
end
